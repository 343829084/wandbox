
.stdin .CodeMirror {
  border: 1px solid #888;
  min-height: 50px;
  height: auto;
}
.stdin .CodeMirror-scroll {
  overflow-y: hidden;
  overflow-x: auto;
}
.stdin .CodeMirror pre {
  font-family: 'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', 'source-code-pro', 'Courier New', monospace;
  font-size: 12px;
  line-height: 16px;
}
